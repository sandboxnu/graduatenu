{
  "name": "@graduate/frontend",
  "version": "1.0.0",
  "description": "A degree scheduling service for Northeastern students.",
  "engines": {
    "node": ">= 12.x"
  },
  "private": true,
  "proxy": "http://localhost:3001",
  "scripts": {
    "dev": "SKIP_PREFLIGHT_CHECK=true react-scripts start start",
    "start": "yarn build && serve -s build",
    "build": "yarn g:cross-env CI=false SKIP_PREFLIGHT_CHECK=true react-scripts build",
    "docker:build": "docker build --progress=plain -t frontend -f Dockerfile ../../",
    "docker:run": "docker run -p 3000:80 frontend",
    "docker:start": "yarn docker:build && yarn docker:run",
    "eject": "react-scripts eject",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@babel/helper-call-delegate": "^7.12.13",
    "@babel/runtime": "^7.17.9",
    "@graduate/api-client": "workspace:^",
    "@graduate/common": "*",
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "@material-ui/lab": "^4.0.0-alpha.61",
    "@material-ui/styles": "^4.11.5",
    "apollo-boost": "^0.4.9",
    "formik": "^2.2.9",
    "graphql": "^16",
    "graphql-tag": "^2.12.6",
    "immer": "^9.0.12",
    "js-cookie": "^3.0.1",
    "js-levenshtein": "^1.1.6",
    "lodash": "^4.17.21",
    "react": "^18.0.0",
    "react-apollo": "^3.1.5",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "^18.0.0",
    "react-error-boundary": "^3.1.4",
    "react-idle-timer": "^4.6.4",
    "react-loader-spinner": "^5.1.4",
    "react-redux": "^7.2.8",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "^5.0.0",
    "react-toast-notifications": "^2.5.1",
    "redux": "^4.1.2",
    "redux-devtools-extension": "^2.13.8",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.4.1",
    "request-promise": "^4.2.6",
    "styled-components": "^5.3.5",
    "timeago.js": "^4.0.2",
    "typesafe-actions": "^5.1.0",
    "xlsx": "^0.15.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.1",
    "@types/lodash": "^4.14.181",
    "@types/node": "^17.0.23",
    "@types/react-beautiful-dnd": "^13.1.2",
    "@types/react-loader-spinner": "^3.1.3",
    "@types/react-redux": "^7.1.23",
    "@types/react-router": "^5.1.18",
    "@types/react-router-dom": "^5.3.3",
    "@types/react-toast-notifications": "^2.4.0",
    "@types/redux-logger": "^3.0.9",
    "@types/styled-components": "^5.1.25",
    "husky": "^7.0.4",
    "typescript": "^4.6.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
